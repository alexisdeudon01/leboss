{
  "version": "0.2.0",
  "configurations": [
    {
      "name": "debugpy: test.py (envMaster)",
      "type": "debugpy",
      "request": "launch",
      "program": "${workspaceFolder}/test.py",
      "console": "integratedTerminal",
      "cwd": "${workspaceFolder}",
      "python": "${workspaceFolder}/envMaster/Scripts/python.exe",
      "env": {
        "PYTHONPATH": "${workspaceFolder}/.."
      }
    },
    {
      "name": "debugpy: current file (envMaster)",
      "type": "debugpy",
      "request": "launch",
      "program": "${file}",
      "console": "integratedTerminal",
      "cwd": "${workspaceFolder}",
      "python": "${workspaceFolder}/envMaster/Scripts/python.exe",
      "env": {
        "PYTHONPATH": "${workspaceFolder}/.."
      }
    }
  ]
}
